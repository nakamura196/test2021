                        <value>sampling</value>
                        <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(sampling) </a:documentation>
                        <data type="token">
                           <param name="pattern">[^\p{C}\p{Z}]+</param>
