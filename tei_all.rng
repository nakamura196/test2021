            <ref name="tei_teiHeader"/>
            <choice>
	              <group>
	                 <oneOrMore>
                     <ref name="tei_model.resource"/>
                  </oneOrMore>
	                 <zeroOrMore>
                     <ref name="tei_TEI"/>
