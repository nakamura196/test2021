            <attribute xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                       name="type"
                       a:defaultValue="audio">
               <a:documentation>録音，録画の種類．</a:documentation>
               <choice>
                  <value>audio</value>
                  <a:documentation>録音．</a:documentation>
                  <value>video</value>
                  <a:documentation>録音，録画．</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
