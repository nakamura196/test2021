               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">当該素性名が定義されているユニコードの版番号を示す．</a:documentation>
               <data type="token">
                  <param name="pattern">[\d]+(\.[\d]+){0,2}</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <define name="tei_localName">
