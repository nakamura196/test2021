               <choice>
                  <ref name="tei_model.rdgLike"/>
                  <ref name="tei_model.noteLike"/>
                  <ref name="tei_witDetail"/>
                  <ref name="tei_wit"/>      
                  <ref name="tei_rdgGrp"/>      
               </choice>
            </zeroOrMore>
         </group>
         <ref name="tei_att.global.attributes"/>
         <ref name="tei_att.typed.attribute.subtype"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">当該要素で示される対象を分類する．</a:documentation>
               <data type="token">
                  <param name="pattern">[^\p{C}\p{Z}]+</param>
               </data>
            </attribute>
         </optional>
